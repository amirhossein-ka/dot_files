st.font:         JetBrains Mono:pixelsize=15:antialias:true
st.font2:        DroidSansMono NerdFont:style:medium:pixelsize=14
st.alpha:        0.90


Xcursor.theme: volantes_cursors




! X colors.
! Generated by 'wal'
*foreground:        #c8cbcc
*background:        #0f191b
*.foreground:       #c8cbcc
*.background:       #0f191b
emacs*foreground:   #c8cbcc
emacs*background:   #0f191b
URxvt*foreground:   #c8cbcc
XTerm*foreground:   #c8cbcc
UXTerm*foreground:  #c8cbcc
URxvt*background:   [100]#0f191b
XTerm*background:   #0f191b
UXTerm*background:  #0f191b
URxvt*cursorColor:  #c8cbcc
XTerm*cursorColor:  #c8cbcc
UXTerm*cursorColor: #c8cbcc
URxvt*borderColor:  [100]#0f191b

